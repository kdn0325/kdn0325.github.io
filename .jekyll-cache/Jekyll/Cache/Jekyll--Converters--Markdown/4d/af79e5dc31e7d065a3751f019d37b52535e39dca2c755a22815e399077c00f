    <span class="nx">result</span><span class="o">=</span><span class="nx">numbers</span><span class="p">.</span><span class="nx">reduce</span><span class="p">((</span><span class="nx">current</span><span class="p">,</span><span class="nx">acc</span><span class="p">)</span><span class="o">=&gt;</span><span class="nx">current</span><span class="o">+</span><span class="nx">acc</span><span class="p">);</span>
    <span class="c1">//reduce = í˜„ì¬ê°’ + ëˆ„ì ê°’</span>
    <span class="nx">console</span><span class="p">.</span><span class="nx">log</span><span class="p">(</span><span class="nx">result</span><span class="p">);</span>
  <span class="p">}</span>
  <span class="nx">add</span><span class="p">(</span><span class="mi">10</span><span class="p">,</span><span class="mi">20</span><span class="p">,</span><span class="mi">30</span><span class="p">,</span><span class="mi">40</span><span class="p">,</span><span class="mi">50</span><span class="p">)</span> 
<span class="p">}</span>

<span class="o">=&gt;</span> <span class="nx">ê²°ê³¼ê°’</span>

