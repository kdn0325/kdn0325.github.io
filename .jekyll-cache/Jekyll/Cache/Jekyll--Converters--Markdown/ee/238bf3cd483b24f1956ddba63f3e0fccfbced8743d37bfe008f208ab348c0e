  <span class="nx">count</span><span class="o">++</span><span class="p">;</span>
  <span class="nb">document</span><span class="p">.</span><span class="nx">querySelector</span><span class="p">(</span><span class="dl">'</span><span class="s1">.box</span><span class="dl">'</span><span class="p">).</span><span class="nx">innerHTML</span><span class="o">=</span><span class="nx">count</span>
<span class="p">}</span>
<span class="nx">start</span><span class="p">.</span><span class="nx">addEventListener</span><span class="p">(</span><span class="dl">'</span><span class="s1">click</span><span class="dl">'</span><span class="p">,()</span><span class="o">=&gt;</span><span class="p">{</span>
  <span class="nx">timeID</span><span class="o">=</span><span class="nx">setInterval</span><span class="p">(</span><span class="nx">make</span><span class="p">,</span><span class="nx">interval</span><span class="p">)</span>
<span class="p">})</span>
