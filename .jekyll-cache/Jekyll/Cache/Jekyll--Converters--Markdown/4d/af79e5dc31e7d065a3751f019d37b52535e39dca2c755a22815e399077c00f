        <span class="p">}</span>
        <span class="nx">getAverage</span><span class="p">(){</span>
            <span class="k">return</span> <span class="k">this</span><span class="p">.</span><span class="nx">getSum</span><span class="p">()</span><span class="o">/</span><span class="mi">4</span>
        <span class="p">}</span>
    <span class="p">}</span>
    <span class="cm">/* í•™ìƒì„ ì„ ì–¸ */</span>
    <span class="kd">const</span> <span class="nx">students</span> <span class="o">=</span> <span class="p">[</span>
